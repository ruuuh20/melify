<div class="container">
  <h1 class="">Spotify Demo</h1>
    <%= form_tag artists_path, method: "get", html: { class: "form" } do %>
     <div class="col-md-4">
       <%= text_field_tag(:artist_name, "", placeholder: "Enter artist name") %>
     </div>
     <%= submit_tag "Search", class: "btn btn-primary" %>
    <% end %>
</div>
